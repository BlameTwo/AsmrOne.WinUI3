    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:cm="using:AsmrOne.WinUI3.Common"
    xmlns:controls="using:AsmrOne.WinUI3.Controls"
    xmlns:local="using:AsmrOne.WinUI3"
    xmlns:wrapper="using:AsmrOne.WinUI3.ViewModels.ItemViewModels">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <XamlControlsResources xmlns="using:Microsoft.UI.Xaml.Controls" />

                <ResourceDictionary Source="Styles/Titlebar.xaml" />
