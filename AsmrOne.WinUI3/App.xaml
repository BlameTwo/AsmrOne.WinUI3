                <ResourceDictionary Source="Styles/HeaderTileStyle.xaml" />
                <ResourceDictionary Source="Styles/AdaptiveView.xaml" />
                <ResourceDictionary Source="Styles/ImageControl.xaml" />
                <!--  Other merged dictionaries here  -->
            </ResourceDictionary.MergedDictionaries>


        </ResourceDictionary>
    </Application.Resources>
</cm:AsmrApplication>
